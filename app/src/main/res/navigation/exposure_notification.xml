<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_diagnosis_submission"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="dev.keiji.cocoa.android.ui.home.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_diagnosisSubmissionFragment"
            app:destination="@id/diagnosisSubmissionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_riskDetailFragment"
            app:destination="@id/riskDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/diagnosisSubmissionFragment"
        android:name="dev.keiji.cocoa.android.exposure_notification.ui.submit_diagnosis.SubmitDiagnosisFragment"
        android:label="DiagnosisSubmissionFragment" />
    <fragment
        android:id="@+id/riskDetailFragment"
        android:name="dev.keiji.cocoa.android.exposure_notification.ui.detect_exposure.RiskDetailFragment"
        android:label="RiskDetailFragment" />
</navigation>
